    function GetCitiesByRegionID(langId, RegionId) {

        var cityparamData = { plangId: langId, pRegionId: RegionId }
        var urls = "/Umbraco/Surface/VisaAPI/GETAllCitiesByRegionID";

        jQuery.ajax({
            type: 'GET',
            url: urls,
            data: cityparamData,
            cache: false,
            dataType: 'JSON',
            contentType: 'application/json',

            success: function(repo) {
               
                if (repo.result.cities != null && repo.result.cities.length > 0) {

                    $("#VisaResidenceCity").html('');
                    $("#VisaResidenceCity").append("<option>select</option>");
                    //Mobile
                    $("#VisaResidenceCitymobile").html('');
                    $("#VisaResidenceCitymobile").append("<option>select</option>");
                    $.each(repo.result.cities, function(indexes, item) {

                        var newListItem = "<option value=\"" + repo.result.cities[indexes].cityId + "\">" + repo.result.cities[indexes].name + "</option>";
                        $("#VisaResidenceCity").append(newListItem);
                        //Mobile
                        $("#VisaResidenceCitymobile").append(newListItem);

                        if ('@CityID' != null && '@CityID' != "" && '@CityID' != "undefined" && '@CityID' == repo.result.cities[indexes].cityId) {
                            $("#VisaResidenceCity option:selected").val(repo.result.cities[indexes].cityId);
                            $("#VisaResidenceCity option:selected").text(repo.result.cities[indexes].name);

                            //MOBILE
                            $("#VisaResidenceCitymobile option:selected").val(repo.result.cities[indexes].cityId);
                            $("#VisaResidenceCitymobile option:selected").text(repo.result.cities[indexes].name);

                        }

                    });
                }
                else { 
                   $("#VisaResidenceCity").html('');
                    $("#VisaResidenceCity").append("<option>NA</option>");
                    //Mobile
                    $("#VisaResidenceCitymobile").html('');
                    $("#VisaResidenceCitymobile").append("<option>NA</option>");
                }

            },
            error: function() {
                alert("Error occurs");
            }
        });
    }

    function CheckVisaRequirments(langId, RegionId, CityId, Destinationid, NationalityId, TravelpurposeId) {

        var ReqVisaparamData = { plangId: langId, pRegionId: RegionId, pCityId: CityId, pDestinationid: Destinationid, pNationalityId: NationalityId, pTravelpurposeId: TravelpurposeId }
        var urls = "/Umbraco/Surface/VisaAPI/GetVisaRequirments";

        jQuery.ajax({
            type: 'GET',
            url: urls,
            data: ReqVisaparamData,
            dataType: 'JSON',
            contentType: 'application/x-www-form-urlencoded; charset=utf-8',
            success: function(data) {

                if (data != null && data.stringObjectValues != null && data.stringObjectValues != "") {

                    $("#apidynamichtml").html(data.stringObjectValues);
                }
                if (data != null && data.titleheading != null && data.titleheading != "") {
                    $("#titlerequrmenttext").css('display', 'block');
                    $("#titlerequrmenttext").append(data.titleheading);
                }

            },
            error: function() {
                alert("Error occurs");
            }
        });

    }

