@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

@{
    Layout = null;
    var NationalID = HttpContextAccessor.HttpContext.Request.Query["nid"];
    var LivingIN = HttpContextAccessor.HttpContext.Request.Query["lvI"];
    var CityID = HttpContextAccessor.HttpContext.Request.Query["citid"];
    var DestinationID = HttpContextAccessor.HttpContext.Request.Query["destid"];
    var TravelingID = HttpContextAccessor.HttpContext.Request.Query["trvlid"];

    string Agent = HttpContextAccessor.HttpContext.Request.Headers["User-Agent"].ToString();
    //string Agent= HttpContextAccessor.HttpContext.Request.Request.Headers["User-Agent"].ToString();
    var BlogNode = Model.Root().Siblings().FirstOrDefault(x => x.ContentType.Alias == "blog");

    //var MainPage = RootSite.DescendantsOrSelf().Where(x => x.ContentType.Alias == "blog").FirstOrDefault();// Model.DescendantsOrSelf<Blog>().FirstOrDefault();
    IList<IPublishedContent> allBlogList = null;
    if (BlogNode != null)
        allBlogList = BlogNode.Children.Where(x => x.ContentType.Alias == "mainCategory").ToList();

    string TestimonialHeading = "";
    if (Model.HasValue("headingText"))
    { TestimonialHeading = Model.Value<string>("headingText"); }

    string TestimonialDescriptons = "";
    if (Model.HasValue("description"))
    { TestimonialDescriptons = Model.Value<string>("description"); }
    //IEnumerable<IPublishedElement>

    var SelectionTestimonials = Model.Value<IEnumerable<IPublishedElement>>("selectTestimonials");
    var SelectionTopRequestedCountries = Model.Value<IEnumerable<IPublishedElement>>("selectTopRequestedCountries");
    string TopRequestedHeading = "";
    if (Model.HasValue("topHeading"))
    { TopRequestedHeading = Model.Value<string>("topHeading"); }

    string ServiceTopHeading = "";
    if (Model.HasValue("serviceTitle"))
    { ServiceTopHeading = Model.Value<string>("serviceTitle"); }

    var SelectionServicesItems = Model.Value<IEnumerable<IPublishedElement>>("serviceItems");

    System.Text.StringBuilder strbgHTML = new System.Text.StringBuilder();
    if (SelectionServicesItems != null && SelectionServicesItems.Count() > 0)
    {
        int countitem = 1;
        foreach (var bgitem in SelectionServicesItems)
        {
            string BannerImage = "";
            var BannerImageMediaPickerSingle = bgitem.Value<IPublishedContent>("backgroundImage");
            if (BannerImageMediaPickerSingle != null)
            {
                BannerImage = BannerImageMediaPickerSingle.Url();
            }
            if (countitem == 1)
            {
                strbgHTML.Append(".header-home {background: url(" + BannerImage + ");}");
            }
            else
            {
                strbgHTML.Append(".header-tab" + countitem + " {background: url(" + BannerImage + ") !important;}");
            }
            countitem++;
        }
    }

     string HeaderTitle = "";
    if (Model.HasValue("title"))
    { HeaderTitle = Model.Value<string>("title"); }

      string HeaderShortintro = "";
    if (Model.HasValue("shortIntro"))
    { HeaderShortintro = Model.Value<string>("shortIntro"); }


      IPublishedContent ChkRequirments = Umbraco.Content(1191);

    IList<IPublishedContent> lstChkRequirments = null;
    if (ChkRequirments != null)
    {
        lstChkRequirments = ChkRequirments.Children.OrderBy(y => y.Name).ToList();
    }

}
